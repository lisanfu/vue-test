{
  "name": "vue-test",
  "version": "1.0.0",
  "description": "从零从零开发一个项目的流程",
  "main": "main.js",
  "repository": "git@github.com:lisanfu/vue-test.git",
  "author": "lisanfu <874359874@qq.com>",
  "license": "MIT",
