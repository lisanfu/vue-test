    "husky": ">=7",
    "lint-staged": ">=10",
    "prettier": "^2.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
