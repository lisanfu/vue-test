  "scripts": {
    "build": "webpack",
    "lint": "npx eslint --ext .js,.jsx,.vue src",
    "lint:fix": "npx eslint --fix --ext .js,.jsx,.vue src",
    "prepare": "husky install",
		"commit": "git-cz"
